{
    "extensions": {
        ".html": {
            "internal": "html",
            "mime": "text/html"
        },
        ".cgi": {
            "internal": "cgi",
            "mime": "text/html"
        },
        ".php": {
            "internal": "php",
            "mime": "text/html",
            "iniPath": "/etc/"
        }
    },
    "indexFile":    "index.html", 
    "docroot":      "./docroot/",
    "handlersPath": "./handlers/",
    "errorPagesPath": "./errors",
    "timeout": 15000
}