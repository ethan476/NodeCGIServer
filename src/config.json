{
    "extensions": {
        ".html": {
            "internal": "html",
            "mime": "text/html"
        },
        ".cgi": {
            "internal": "cgi",
            "mime": "text/html"
        },
        ".php": {
            "internal": "php",
            "mime": "text/html",
            "iniPath": "/etc/"
        },
        ".ico": {
            "internal": "html",
            "mime": "image/x-icon"
        },
        ".css": {
            "internal": "html",
            "mime": "text/css"
        }
        ,
        ".js": {
            "internal": "html",
            "mime": "application/javascript"
        }
    },
    "indexFile":    "index.cgi", 
    "docroot":      "./docroot/",
    "handlersPath": "./handlers/",
    "errorPagesPath": "./errors/",
    "timeout": 15000
}
